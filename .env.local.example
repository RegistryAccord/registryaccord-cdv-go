# Local CDV Service Environment Variables (gitignored)
# Copy this file to .env.local for local development overrides

# Uncomment and set your local configuration values

# PostgreSQL connection string
# CDV_DB_DSN=postgres://user:password@localhost:5432/cdv

# NATS server URL
# CDV_NATS_URL=nats://localhost:4222

# S3-compatible storage endpoint
# CDV_S3_ENDPOINT=http://localhost:9000

# S3 bucket name
# CDV_S3_BUCKET=cdv-media

# S3 access key
# CDV_S3_ACCESS_KEY=minioadmin

# S3 secret key
# CDV_S3_SECRET_KEY=minioadmin

# Identity service URL for DID validation
# IDENTITY_URL=http://localhost:8081
